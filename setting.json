{
  "editor.tabSize": 2,
  "editor.wordWrap": "on",
  "files.autoSave": "afterDelay",
  "editor.formatOnSave": true,
  "editor.fontSize": 16,
  "editor.minimap.enabled": false,
  "workbench.colorTheme": "One Dark Pro",
  "files.associations": {
    "*.js": "javascript",
    "*.json": "json"
  },
  "editor.codeActionsOnSave": {
    "source.fixAll": true
  },
  "liveServer.settings.port": 5500,
  "liveServer.settings.root": "./"
}
